    "import datetime\n",
    "def get_day_nday_ago(date,n):\n",
    "    t = time.strptime(date, \"%Y-%m-%d\")\n",
    "    y, m, d = t[0:3]\n",
    "    Date = str(datetime.datetime(y, m, d) - datetime.timedelta(n)).split()\n",
    "    return Date[0]\n",
    "\n",
    "# 示例\n",
    "a=get_day_nday_ago('2017-02-11',7)\n",
    "print a\n",
    "2017-02-04\n"
   ]
  }
